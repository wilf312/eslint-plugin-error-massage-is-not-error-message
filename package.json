{
  "name": "eslint-plugin-error-massage-is-not-error-message",
  "version": "0.0.2",
  "description": "clean error massage.",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "build": "tsc",
    "test": "jest",
    "prepublish": "npm run clean && npm run build"
  },
  "files": [
    "README.md",
    "package.json",
    "lib"
  ],
  "keywords": [
    "eslint",
    "typescript",
    "massage"
  ],
  "author": "OkadaGenya@WiLF",
  "license": "MIT",
  "devDependencies": {
    "@types/eslint": "7.2.2",
    "@types/node": "12.12.62",
    "@typescript-eslint/parser": "4.1.1",
    "eslint": "7.9.0",
    "jest": "26.4.2",
    "rimraf": "3.0.2",
    "ts-jest": "26.4.0",
    "typescript": "4.0.3"
  },
  "dependencies": {},
  "peerDependencies": {
    "eslint": "^4.0.0 || ^5.0.0 || ^7.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(src/.*\\.test)\\.ts$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "\\.d\\.ts$",
      "lib/.*"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "json"
    ]
  }
}